{
  "name": "@listingslab/hof",
  "version": "19.1.7",
  "scripts": {
    "test": "clear && cd ./packages/appshell && npm test",
    "start": "clear && npm run dev",
    "dev": "$PWD/.scripts/unix/dev",
    "build": "clear && npx lerna run build && npm run dist",
    "dist": "$PWD/.scripts/unix/dist 19.1.7",
    "serve": "clear && serve dist",
    "ssr": "$PWD/.scripts/unix/ssr",
    "functions": "clear && cd ./packages/functions && npm run serve",
    "functions:dev": "clear && cd ./packages/functions && npm run serve",
    "bootstrap": "clear && npx lerna bootstrap",
    "new-app": "clear && npx create-single-spa",
    "lint": "clear && cd ./packages/functions && npm run lint",
    "deploy": "clear && npm run lint && npm run build && firebase deploy"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  }
}
